#!/usr/bin/env python
import math
from paper_folding import PaperFolding
from paper_svg import PaperSvg

def main():
    # Create a new instance of the PaperSvg class
    paper = PaperSvg()

    # Create a new instance of the PaperFolding class
    paper_folding = PaperFolding(width=paper.width, height=paper.height)

    # Apply a series of transformations to the folded paper
    #paper_folding.fold(a=200, alpha=math.pi/6, b=300, beta=math.pi/4)
    paper_folding.apply_random_fold()
    # Set the folded paper state in the PaperSvg class
    print(paper.getOrig())
    print(paper.getMarked())
    print(paper.getFolded())


if __name__ == '__main__':
    main()

